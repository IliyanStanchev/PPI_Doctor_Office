<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="681.0" prefWidth="621.0" xmlns="http://javafx.com/javafx/11.0.2"
            fx:controller="controllers.RegisterPersonalInformationController">
    <top>
        <AnchorPane prefHeight="95.0" prefWidth="550.0" style="-fx-background-color: #58D68D;"
                    BorderPane.alignment="CENTER">
            <children>
                <Text layoutX="159.0" layoutY="61.0" strokeType="OUTSIDE" strokeWidth="0.0" text="User Registration">
                    <font>
                        <Font name="Tw Cen MT" size="45.0"/>
                    </font>
                </Text>
            </children>
        </AnchorPane>
    </top>
    <center>
        <AnchorPane prefHeight="586.0" prefWidth="582.0" BorderPane.alignment="CENTER">
            <children>
                <Label fx:id="emailLabel" alignment="CENTER" layoutX="190.0" layoutY="14.0" prefHeight="45.0"
                       prefWidth="228.0" text="Personal Information">
                    <font>
                        <Font name="System Bold" size="18.0"/>
                    </font>
                </Label>
                <TextField fx:id="firstNameField" layoutX="197.0" layoutY="59.0" onKeyReleased="#onFirstNameReleased"
                           prefHeight="44.0" prefWidth="212.0" promptText="First name"
                           style="-fx-background-color: transparent; -fx-border-color: #58D68D; -fx-border-width: 0px 0px 2px 0px;">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </TextField>
                <TextField fx:id="lastNameField" layoutX="195.0" layoutY="144.0" onKeyReleased="#onLastNameReleased"
                           prefHeight="42.0" prefWidth="216.0" promptText="Last name"
                           style="-fx-background-color: transparent; -fx-border-color: #58D68D; -fx-border-width: 0px 0px 2px 0px;">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </TextField>
                <TextField fx:id="phoneNumberField" layoutX="195.0" layoutY="223.0"
                           onKeyReleased="#onPhoneNumberReleased" prefHeight="42.0" prefWidth="216.0"
                           promptText="Phone number"
                           style="-fx-background-color: transparent; -fx-border-color: #58D68D; -fx-border-width: 0px 0px 2px 0px;">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </TextField>
                <TextField fx:id="personalIdentifierField" layoutX="195.0" layoutY="302.0"
                           onKeyPressed="#onPersonalIdentifierReleased" prefHeight="42.0" prefWidth="216.0"
                           promptText="Personal Identifier"
                           style="-fx-background-color: transparent; -fx-border-color: #58D68D; -fx-border-width: 0px 0px 2px 0px;">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </TextField>
                <DatePicker fx:id="dateOfBirthField" layoutX="192.0" layoutY="387.0" prefHeight="38.0" prefWidth="224.0"
                            promptText="Date of birth" style="-fx-border-color: #58D68D;"/>
                <Button layoutX="48.0" layoutY="485.0" mnemonicParsing="false" onAction="#onRegisterAsPatient"
                        prefHeight="48.0" prefWidth="232.0" style="-fx-background-color: #58D68D;"
                        text="Register as patient">
                    <font>
                        <Font name="Tw Cen MT" size="26.0"/>
                    </font>
                </Button>
                <Button layoutX="341.0" layoutY="485.0" mnemonicParsing="false" onAction="#onApplyAsDoctor"
                        prefHeight="48.0" prefWidth="232.0" style="-fx-background-color: #58D68D;"
                        text="Apply for doctor">
                    <font>
                        <Font name="Tw Cen MT" size="26.0"/>
                    </font>
                </Button>
                <Label fx:id="firstNameLabel" layoutX="197.0" layoutY="103.0" prefHeight="18.0" prefWidth="210.0"
                       textFill="RED"/>
                <Label fx:id="lastNameLabel" layoutX="196.0" layoutY="186.0" prefHeight="18.0" prefWidth="214.0"
                       textFill="RED"/>
                <Label fx:id="phoneNumberLabel" layoutX="192.0" layoutY="265.0" prefHeight="18.0" prefWidth="218.0"
                       textFill="RED"/>
                <Label fx:id="personalIdentifierLabel" layoutX="192.0" layoutY="344.0" prefHeight="18.0"
                       prefWidth="218.0" textFill="RED"/>
                <Label fx:id="dateOfBirthLabel" layoutX="194.0" layoutY="425.0" prefHeight="18.0" prefWidth="31.0"
                       textFill="RED"/>
                <Label fx:id="resultLabel" layoutX="55.0" layoutY="449.0" prefHeight="18.0" prefWidth="490.0"
                       textFill="RED"/>
                <ImageView fitHeight="48.0" fitWidth="52.0" layoutX="14.0" layoutY="14.0" onMouseClicked="#onGoBack"
                           pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@icons/Back.png"/>
                    </image>
                </ImageView>
                <Label layoutX="297.0" layoutY="459.0" textFill="RED"/>
            </children>
        </AnchorPane>
    </center>
</BorderPane>
